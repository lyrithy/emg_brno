<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.8/css/all.css">


<div class="container">
<br><p class="text-center">Emergency List</p>
<hr>

<div class="row">
	<aside class="col-sm-4">


            <div class="card">
                    <article class="card-group-item">
                        <header class="card-header"><h6 class="title">Emergency(Universal European) </h6></header>
                        <div class="filter-content">
                            <div class="list-group list-group-flush">
                             
                                <% for (var i in issues) { %>
                                    <% if (issues[i].category == 'emgEU') { %>
                                    <!-- http://www.google.com/maps/place/49.46800006494457,17.11514008755796 -->
                                    
                                    
                                    <a href="http://www.google.com/maps/place/<%= issues[i].geo_location.lat %>,<%= issues[i].geo_location.lng %>" class="list-group-item"><%= issues[i].name %> 
                                        <span class="float-right badge badge-light round"> Tel:<%= issues[i].phone %></span> 
                                        <span class="float-right badge badge-light round">Email:<%= issues[i].email %></span>
                                     </a>
                                            
                                  <% }} %> 
                            
                            </div>  <!-- list-group .// -->
                        </div>
                    </article> <!-- card-group-item.// -->	
                </div> <!-- card.// -->




    </aside> <!-- col1.// -->    
	<aside class="col-sm-4">



<div class="card">
	<article class="card-group-item">
		<header class="card-header"><h6 class="title">Police(City) </h6></header>
		<div class="filter-content">
			<div class="list-group list-group-flush">
                <% for (var i in issues) { %>
                    <% if (issues[i].category == 'emgPolice') { %>
                    <!-- http://www.google.com/maps/place/49.46800006494457,17.11514008755796 -->
                    
                    
                    <a href="http://www.google.com/maps/place/<%= issues[i].geo_location.lat %>,<%= issues[i].geo_location.lng %>" class="list-group-item"><%= issues[i].name %> 
                        <span class="float-right badge badge-light round"> Tel:<%= issues[i].phone %></span> 
                        <span class="float-right badge badge-light round">Email:<%= issues[i].email %></span>
                     </a>
                            
                  <% }} %>  
            
            
            </div>  <!-- list-group .// -->
		</div>
	</article> <!-- card-group-item.// -->	
</div> <!-- card.// -->



	</aside> <!-- col2.// -->
	<aside class="col-sm-4">

            <div class="card">
                    <article class="card-group-item">
                        <header class="card-header"><h6 class="title">Ambulance </h6></header>
                        <div class="filter-content">
                            <div class="list-group list-group-flush">
                                <% for (var i in issues) { %>
                                    <% if (issues[i].category == 'emgAmbulance') { %>
                                    <!-- http://www.google.com/maps/place/49.46800006494457,17.11514008755796 -->
                                    
                                    
                                    <a href="http://www.google.com/maps/place/<%= issues[i].geo_location.lat %>,<%= issues[i].geo_location.lng %>" class="list-group-item"><%= issues[i].name %> 
                                        <span class="float-right badge badge-light round"> Tel:<%= issues[i].phone %></span> 
                                        <span class="float-right badge badge-light round">Email:<%= issues[i].email %></span>
                                     </a>
                                            
                                  <% }} %>                             
                                </div>  <!-- list-group .// -->
                        </div>
                    </article> <!-- card-group-item.// -->	
                </div> <!-- card.// -->


    </aside> <!-- col.// -->
    
    
</div> <!-- row.// -->
<br>
<div class="row">
        <aside class="col-sm-8">
              

                <div class="card">
                        <article class="card-group-item">
                            <header class="card-header"><h6 class="title">Report Problems </h6></header>
                            <div class="filter-content">
                                <div class="list-group list-group-flush">
