                                        <span class="float-right badge badge-light round"> Tel:<%= issues[i].phone %></span> 
                                        <span class="float-right badge badge-light round">Email:<%= issues[i].email %></span>
                                     </a>
                                  		  
                                  <% }} %>
                                </div>  <!-- list-group .// -->
                            </div>
                        </article> <!-- card-group-item.// -->	
                    </div> <!-- card.// -->
    
    
    
    
        </aside> <!-- col1.// -->    
        <aside class="col-sm-4">


                <div class="card">
                        <article class="card-group-item">
                            <header class="card-header"><h6 class="title">Fire </h6></header>
                            <div class="filter-content">
                                <div class="list-group list-group-flush">
                                    <% for (var i in issues) { %>
