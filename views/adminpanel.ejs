                                      <% }} %>      
                                    </div>  <!-- list-group .// -->
                            </div>
                        </article> <!-- card-group-item.// -->	
                    </div> <!-- card.// -->
    
    
    
    
        </aside> <!-- col1.// -->    

</div>

